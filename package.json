{
  "name": "ad-highlighter",
  "version": "1.0.0",
  "description": "A Chrome extension that highlights ads on web pages with colored borders to make them easily identifiable.",
  "main": "dist/content.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "clean": "rimraf dist",
    "package": "npm run build && cd dist && zip -r ../ad-highlighter-extension.zip .",
    "test-build": "cd /src && node test-build.js"
  },
  "keywords": [
    "chrome-extension",
    "ads",
    "highlighter"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "html-webpack-plugin": "^5.6.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
